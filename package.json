{
  "name": "devassist_ai",
  "displayName": "devassist_ai",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "assets/icons/icon.png",
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "devassist_ai-ActivityBar",
          "title": "DevAssist AI",
          "icon": "assets/icons/icon.svg"
        }
      ]
    },
    "views": {
      "devassist_ai-ActivityBar": [
        {
          "type": "webview",
          "id": "devassist_ai.SidebarProvider",
          "name": ""
        }
      ]
    },
    "commands": [
      {
        "command": "devassist_ai.buttonClicked",
        "title": "New Task",
        "icon": "$(add)"
      },
      {
        "command": "devassist_ai.popoutButtonClicked",
        "title": "Open in Editor",
        "icon": "$(link-external)"
      },
      {
        "command": "devassist_ai.settingsButtonClicked",
        "title": "Settings",
        "icon": "$(settings-gear)"
      },
      {
        "command": "devassist_ai.openInNewTab",
        "title": "Open In New Tab",
        "category": "DevAssist AI"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test",
    "install:all": "npm install && cd webview-ui && npm install",
    "start:webview": "cd webview-ui && npm run start",
    "build:webview": "cd webview-ui && npm run build",
    "test:webview": "cd webview-ui && npm run test"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.30.1",
    "default-shell": "^2.2.0",
    "delay": "^6.0.0",
    "diff": "^5.2.0",
    "os-name": "^6.0.0",
    "@google/generative-ai": "^0.21.0",
    "openai": "^4.68.1"
  },
  "devDependencies": {
    "@types/diff": "^5.2.3",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.94.0",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "esbuild": "^0.23.1",
    "eslint": "^9.9.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.5.4"
  }
}
