{
  "name": "chatbot-file-explorer",
  "displayName": "Chatbot File Explorer",
  "description": "VS Code extension with chatbot, file explorer, and terminal control",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:chatbot-file-explorer.openChatbot"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "chatbot-file-explorer.openChatbot",
        "title": "Open Chatbot"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.60.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  }
}